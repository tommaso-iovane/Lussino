meta {
  name: Create User
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/auth/create-user
  body: json
  auth: none
}

body:json {
  {
    "username": "testuser",
    "email": "test@example.com",
    "password": "securepassword123"
  }
}

assert {
  res.status: eq 201
  res.body.message: eq User created successfully
  res.body.user.username: eq testuser
  res.body.user.email: eq test@example.com
}