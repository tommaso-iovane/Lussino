meta {
  name: Update Password
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/api/auth/update-password
  body: json
  auth: none
}

headers {
  Cookie: {{sessionCookie}}
}

body:json {
  {
    "currentPassword": "changeme1",
    "newPassword": "changeme"
  }
}

assert {
  res.status: eq 200
  res.body.message: eq Password updated successfully
}
